#!/bin/bash

if [ -z "${XNAT_PBS_JOBS}" ]; then
	echo "$(basename ${0}): ABORTING: XNAT_PBS_JOBS environment variable must be set"
	exit 1
fi

source activate python3 2>/dev/null
${XNAT_PBS_JOBS}/7T/RepairIcaFixProcessingHCP7T/CheckRepairIcaFixProcessingHCP7TCompletionBatch.py $@
source deactivate 2>/dev/null

